{
  "name": "corella",
  "private": true,
  "version": "0.0.2",
  "description": "Core UI components for Corella Design System",
  "author": "German Aviles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Agercho/corella.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "storybook": "turbo --filter storybook dev",
    "install-all": "turbo install",
    "clean": "turbo clean",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "prettier": "^3.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "esbuild": "0.21.5"
    }
  }
}
